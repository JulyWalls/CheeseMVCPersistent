
@model List<CheeseMVC.Models.CheeseCategory>
@{
    ViewData["Title"] = "Index";
}

<h2>Category Names</h2>
@if (Model.Count == 0)
{
    <p>No Cheese Categories Yet!</p>

}
<ul>
@foreach (var cat in Model)
{
    <li>@cat.Name</li>

}
</ul>

    <p><a asp-controller="Category" asp-action="Add">Add Category</a></p>
